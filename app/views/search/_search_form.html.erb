  <%= form_tag search_twitter_path, method: "get", class: "form-inline", id: "tweet_search" do %>
    <%= text_field_tag :search, params[:search], class: "span4", id: "instant_search", onkeyup: "showResult(this.value)" %>
    <%= submit_tag "Search", class: "btn btn-primary", id: "JSButton" %>
  <% end %>